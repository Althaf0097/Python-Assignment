   "source": [
    "###  9.Display all duplicate items from a list:\n",
    "10 20 60 30 40 30 60 70 80"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "2ec73266",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[20, 30, 60]\n"
     ]
    }
   ],
   "source": [
    "list1=[10,20,60,30,20,40,30,60,70,80]\n",
    "lis=[]\n",
    "duplicate=[]\n",
    "for i in list1:\n",
    "    if i not in lis:\n",
    "        lis.append(i)\n",
    "    elif i not in duplicate:\n",
    "        duplicate.append(i)\n",
    "        \n",
    "print(duplicate)\n"
   ]
  },
