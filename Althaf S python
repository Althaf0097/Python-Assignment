   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7b459982",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Temperature value in degree Celsius: 30\n",
      "The 30.00 degree Celsius is equal to: 86.00 Fahrenheit\n",
      "Temperature value in Fahrenheit: 86\n",
      "The 86.00 degree Fahrenheit is equal to: 30.00 celsius\n"
     ]
    }
   ],
   "source": [
    "celsius_1 = float(input(\"Temperature value in degree Celsius: \" ))   \n",
    "Fahrenheit_1 = (celsius_1 * 1.8) + 32  \n",
    "print('The %.2f degree Celsius is equal to: %.2f Fahrenheit'%(celsius_1, Fahrenheit_1))\n",
    "\n",
    "\n",
    "Fahrenheit_2=float(input(\"Temperature value in Fahrenheit: \"))\n",
    "celsius_2= (Fahrenheit_2-32)/1.8\n",
    "print('The %.2f degree Fahrenheit is equal to: %.2f celsius'%(Fahrenheit_2, celsius_2))"
