    "### 7. Write a python program to construt the following pattern using a nested for loop\n",
    "*  \n",
    "\n",
    "*  *  \n",
    "\n",
    "*  *  *  \n",
    "\n",
    "*  *  *  *  \n",
    "\n",
    "*  *  *  *  *  \n",
    "\n",
    "*  *  *  *  \n",
    "\n",
    "*  *  *  \n",
    "\n",
    "*  *  \n",
    "\n",
    "*  \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "d64ec5bf",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "*  \n",
      "\n",
      "*  *  \n",
      "\n",
      "*  *  *  \n",
      "\n",
      "*  *  *  *  \n",
      "\n",
      "*  *  *  *  *  \n",
      "\n",
      "*  *  *  *  \n",
      "\n",
      "*  *  *  \n",
      "\n",
      "*  *  \n",
      "\n",
      "*  \n",
      "\n",
      "\n",
      "\n"
     ]
    }
   ],
   "source": [
    "rows = 5\n",
    "for i in range(rows):\n",
    "    for j in range(i+1):\n",
    "        print(\"* \",end=\" \")\n",
    "    print(\"\\n\")\n",
    "for i in range(rows,0,-1):\n",
    "    for j in range(1,i):\n",
    "        print(\"* \",end=\" \")\n",
    "    print(\"\\n\")\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e8cfacb0",
